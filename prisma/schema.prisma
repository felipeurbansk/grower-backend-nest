// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Grows {
  id Int @default(autoincrement()) @id
  name String
  height Int
  width Int
  depth Int
  status String?
}

model Seeds {
  id Int @default(autoincrement()) @id
  name String
  bank_name String?
  thc Int?
  cbd Int?
  vegetative_weeks Int?
  flowering_weeks Int?
  per_square_meter Int?
  status String?
}

model Lights {
  id Int @default(autoincrement()) @id
  name String
  watts Int?
  lumens Int?
  status String?
}

model BaseComponents {
  id Int @default(autoincrement()) @id
  ip String
  mac String
  temperatures Temperatures[]
  humidities Humidities[]
}
